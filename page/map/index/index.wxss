/* index.wxss */
map{
  width:100%;
  height:100vh;
  padding:0;
  margin:0;
}
.mapinfo{
  height:40px;
  width:100%;
  display:flex;
  line-height:40px;
  background-color:#ffffff;
  font-size:10pt;
  border-bottom:1px solid #cacaca;
}
.item-10{
  width:10%;
}
.item-70{
  width:70%;
}
.item-20{
  width:20%;
}
.tip{
  width:25px;height:25px;vertical-align: middle;padding-right:5px;border-right:1px solid #CACACA;padding-left:5px;
}
.tip3{
  width:20px;height:20px;vertical-align: middle;padding-left:5px;
}
.radius_normal{
  text-align: center;
  background-color:#1296db;
  border-radius:5px;
  height:30px;
  line-height: 30px;
  margin-top:4px;
  width:90%;
  border:1px solid #cdcdcd;
  color:#ffffff;
}