    <view class="items borderbtm">
      <view class="item-40 bright">消防车辆（辆）</view>
      <view class="item-60">
        <input type='number' value='0'></input>
      </view>
    </view>
    <view class="items">
      <view class="item-40 bright">故障车辆（辆）</view>
      <view class="item-60">
        <input type='number' value='0'></input>
      </view>
    </view>
  </view>
  <view style='width:90%;margin:10px auto'>
    <button type="primary" style="font-size:10pt;background-color:#2782D7" bindtap="submitCar">提交</button>
  </view>
</view>