      <view class="item-60">
        <input type='number' value='0'></input>
      </view>
    </view>
  </view>
  <view class="dutys">
    <view class="items borderbtm">
      <view class="item-10 center">
        <image src="../../../image/dpt.png" class='tip'></image>
      </view>
      <view class="item-40 left">麒麟一中队</view>
      <view class="item-40"></view>
