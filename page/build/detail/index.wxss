/* index.wxss */
@import "../../common/lib/weui.wxss";
.list_flex-wrp{
  display:flex;
  padding-left:5px;
  padding-right:5px;
  padding-top:5px;
  background-color:#ffffff;
}
.build_list_pic{
  padding:5px;
  border-bottom:1px solid #EAEAEA;
}
.build_list_pic image{
  width:100%;
  height:200px;
}
.flex-item{
  font-size: 10pt;
  padding-bottom:5px;
  border-bottom:1px solid #dcdcdc;
}
.item-10{
  width:10%;
}
.item-20{
  width:20%;
}
.item-80{
  width:80%;
}
.item-90{
  width:90%;
  text-align: left;
}
.tip{
  width:20px;height:20px;vertical-align: middle;padding-right:5px;border-right:1px solid #CACACA;
}
.tip2{
  width:20px;height:20px;vertical-align: middle;padding-right:2px;
}
.tip3{
  width:20px;height:20px;vertical-align: middle;border-left:1px solid #CACACA;padding-left:5px;
}
.radius{
  background-color:#EAEAEA;
  border-radius:5px;
  padding-left:5px;
  padding-right:5px;
  padding-bottom:2px;
  float:left;
  border:1px solid #373937;
}
.file_title{
  background-color: #373937;
  color:white;
}
.file_link{
  height:20px;
  overflow-y: hidden;
  overflow-x: hidden;
  margin-top:3px;
}
.file_hide{
  display:none;
}