/* index.wxss */
@import "../../common/lib/weui.wxss";
.list_flex-wrp{
  display:flex;
  padding-left:5px;
  padding-right:5px;
  padding-top:5px;
  background-color:#ffffff;
}
.list_flex-wrp2{
  display:flex;
  padding-right:5px;
  background-color:#ffffff;
}
.build_list_pic{
  padding:5px;
  border-bottom:1px solid #EAEAEA;
}
.build_list_pic image{
  width:100%;
  height:200px;
}
.flex-item{
  font-size: 10pt;
  padding-bottom:5px;
  border-bottom:1px solid #dcdcdc;
}
.flex-item2{
  font-size: 10pt;
  padding-top:5px;
}
.flex-item3{
  font-size: 10pt;
  padding-bottom:5px;
}
.item-10{
  width:5%;
}
.item-20{
  width:20%;
}
.item-25{
  width:25%;
  color:#888;
}
.item-80{
  width:80%;
}
.item-75{
  width:75%;
}
.item-50{
  width:50%;
}
.item-90{
  width:95%;
  text-align: left;
}
.item-auto{
  text-align: left;
}
.tip{
  width:20px;height:20px;vertical-align: middle;padding-right:5px;border-right:1px solid #CACACA;
}
.tip2{
  width:20px;height:20px;vertical-align: middle;padding-right:2px;
}
.tip3{
  width:20px;height:20px;vertical-align: middle;border-left:1px solid #CACACA;padding-left:5px;
}
.tip5{
  width:20px;height:20px;vertical-align: middle;
  margin-left:-20px;
  margin-top:10px;
}
.line{
  border-left:3px solid #1296db;
  text-align: center;
  margin-left:15px;
}
.radius{
  background-color:#EAEAEA;
  border-radius:5px;
  padding-left:5px;
  padding-right:5px;
  padding-bottom:2px;
  float:left;
  border:1px solid #373937;
}
.file_title{
  background-color: #373937;
  color:white;
}
.file_link{
  height:20px;
  overflow-y: hidden;
  overflow-x: hidden;
}
.flex-item text{
  border-right:1px solid #CACACA;
  padding-right:5px;
}

.font-fade{
  font-size: 9pt;
  color:#888;
  margin-top:10px;
}
.weui-cells{
  border-bottom:0;
  border-top:0;
}
.plan_input{
  text-align: left;
  background-color:#fff;
  font-size: 10pt;
}
.plan_input textarea{
  width:100%;
  height:60px;
  line-height: 10px;
}
.video_padding{
  padding-top:10px;
  background-color:#fff;
  text-align: center;
}
.video{
  width:100%;
  height:200px;
}
.file_hide{
  display:none;
}
.divShow{
  display: block;
}
.divHide{
  display: none;
}
.worknote_img{
  width:100%;display:flex-inline;text-align: left;
}
.worknote_img image{
  width:90px;height:90px;padding-right:5px;padding-top:5px;
}