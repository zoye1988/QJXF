.contain{
  margin:0 auto;
  padding-top:5px;
}
.main{
  width:95%;
  margin:0 auto;
  background-color:#ffffff;
  font-size: 10pt;
}
.best{
  width:99%;
  margin:0 auto;
  height:150px;
  border-radius: 10px;
  align-items: center;
  background-color:#ffffff;
  border:1px solid #969696;
  margin-top:5px;
}
.rank{
  margin:0 auto;
  margin-top:5px;
  width:98%;
}
.bk{
  border-bottom:1px dotted #cdcdcd;
}
.br{
  border-right:1px solid #cdcdcd;
}
image{
  width:30px;
  height:30px;
}
.border-btm{
  border-bottom:1px solid #cdcdcd;
  padding-bottom:5px;
}
.item{
  display: flex;
  align-items: center;
  height:30px;
  line-height: 20px;
  width:99%;
  margin:0 auto;
  text-align: center;
}
.item2{
  display: flex;
  align-items: center;
  height:100px;
  line-height: 90px;
  width:99%;
  margin:0 auto;
  font-size: 30pt;
  text-align: center;
}
._item{
  display: flex;
  align-items: center;
  line-height: 20px;
  width:99%;
  margin:0 auto;
  text-align: center;
}
.items{
  display: flex;
  align-items: center;
  margin-left:5px;
  margin-right:5px;
}
