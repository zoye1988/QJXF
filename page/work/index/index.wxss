/* index.wxss */
@import "../../common/lib/weui.wxss";
.radio {
  margin-right: 20rpx;
}
.page-section{
  margin-bottom:0;
}
.flex-wrp{
  margin-top:0px;
  display:flex;
  height:35px;
  border-bottom:1px solid #CACACA;
}
.flex-item{
  font-size: 26rpx;
}
.search_item{
  width:90%;
  height:35px;
  padding-top:5px;
}
.wxsearch{
  width:100%;
  height:30px;
  font-size: 10pt;
  padding-left:5px;
}
.search_text{
  width:13%;
  height:25px;
  text-align: center;
  border-right:1px solid #CACACA;
  margin-top:5px;

}
.search_icon{
  width:10%;
  height:25px;
  text-align: center;
  margin-top:5px;
  border-left:1px solid #CACACA;
}
.searchImg{
  width:25px;
  height:25px;
}
.formImg{
  width:25px;
  height:25px;
}
.search_spacing{
  margin:0px;
  padding-left:1px;
  padding-right:1px;
  background-color:white;
  border-bottom:1px solid #cacaca;
}
.showItemCss{
  display: block;
}
.hideItemCss{
  display: none;
}
.chemical_flex-wrp{
  display:flex;
  background-color:#ffffff;
  border-bottom:1px solid #CACACA;
  padding-top:5px;
  padding-bottom:5px;
}
.clist-flex-wrp{
  display:flex;
  line-height: 20px;
  width:98%;
}
.date_view{
    color:#888;
    font-size:9pt;
    margin-top:5px;
    text-align: left;

}
.image_view image{
  width:70px;height:70px;
  padding:2px;
}
.danger-flex-wrp{
  margin-right:10px;
  color: black;
  font-size:10pt;
}
.list_flex-item{
  font-size: 26rpx;
}
.buildImg{
  width:130px;
  height:80px;
  margin-top:5px;
}
button{
  margin-left:5px;
  margin-right:5px;
  background-color:#373937;
  color:#ffffff;
  font-size:11pt;
}
.button-sp-area{
  margin: 0 auto;
  width: 60%;
}
.mini-btn{
  margin-right: 10rpx;
}
.build_content{
  width:85%;
}
.chemical_title{
  line-height: 20px;
  height:20px;
  font-size: 10pt;
  overflow-y: hidden;
  margin-top:5px;
  color:#1296db;
}
.chemical_title2{
  font-size: 10pt;
  height:30px;
  overflow-y: hidden;
}
.build_address{
  font-size: 10pt;
  height:40px;
  overflow-y: hidden;
  padding-top:7px;
  color:#888;
  border-top:1px solid #CACACA;
  width:100%;
}
.build_type{
  font-size: 10pt;
  padding-top:7px;
  height:20px;
  color:#888;
  width:100%;
}
.tip{
  width:15px;
  height:15px;
  vertical-align: middle;
  border-right:1px solid #CACACA;
  padding-right:5px;
  margin-right:5px;
}
.item_txt{
  float:left;
  vertical-align: middle;
  height:20px;
  margin-left: 5px;
}
.radius_normal{
  background-color:#80a0fe;
  border-radius:5px;
  padding-left:5px;
  padding-right:5px;
  float:left;
  border:1px solid #373937;
  color:#ffffff;
}
.radius_broke{
  background-color:#fb6570;
  border-radius:5px;
  padding-left:5px;
  padding-right:5px;
  float:left;
  border:1px solid #373937;
  color:#ffffff;
}
.item-intense{
  float:left;
}
.item-10{
  width:10%;
}
.item-13{
  width:13%;
}
.item-15{
  width:15%;
}
.item-20{
  width:20%;
}
.item-30{
  width:30%;
}
.item-40{
  width:40%;
}
.item-60{
  width:60%;
}
.item-35{
  width:35%;
}
.item-80{
  width:80%;
}
.item-70{
  width:70%;
}
.item-90{
  width:90%;
  text-align: left;
}
.home_image{
  width:25px;
  height:25px;
}
.note_btn_icon{
  width:20px;
  height:30px;
  text-align:center;
}
.note_icon{
  width:30px;
  height:30px;
  text-align:center;
  border-right:1px solid #888;
  padding-right:10px;
  margin-top:10px;
}
.chemical-tip{
  margin-right:5px;
  text-align: center;
}
.chemical-txt{
  font-size:10pt;
}
.icon-box2{
  padding: 0;
  margin-right:10px;
  display: flex;
  align-items: center;
  background-color:#fcfcfc;
  border:1px solid #dcdcdc;
  height:100px; 
}
.icon-box-img{
  margin-right: 46rpx;
}
.icon-box-ctn{
  flex-shrink: 100;
  padding-left: 10px;
  margin-top:4px;
  margin-bottom:4px;
  width:85%;
}
.icon-box-title{
  font-size: 10pt;
  font-weight: bold;
  color:#888;
}
.icon-box-desc{
  margin-top: 2px;
  font-size: 10pt;
  color: #888;
  height:40px;
  overflow-y: hidden;
}
.icon-box-desc2{
  margin-top: 2px;
  font-size: 10pt;
  color: #888;
  height:65px;
  overflow-y: hidden;
}
.worknote_img{
  width:100%;display:flex-inline;text-align: left;
}
.worknote_img image{
  width:80px;height:80px;padding-right:5px;padding-top:5px;
}